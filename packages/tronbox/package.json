    "truffle-contract": "^3.0.6",
    "truffle-core": "^4.1.14",
    "web3": "0.20.6",
    "webpack": "^2.5.1",
    "yargs": "^8.0.2"
  },
  "bin": {
    "tronbox": "./build/cli.bundled.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build-tronweb && npm run build-cli",
    "build-tronweb": "cd ../tronwrap/tron-web && yarn build -p",
