{
  "name": "tronbox",
  "namespace": "tronprotocol",
  "version": "2.4.0",
  "description": "TronBox - Simple development framework for tronweb",
  "dependencies": {
    "mocha": "^4.1.2",
    "original-require": "1.0.1"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "fs-extra": "6.0.1",
    "ganache-cli": "6.1.0-beta.4",
    "glob": "^7.1.2",
